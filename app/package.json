{
  "name": "stem-zipper-electron",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main/main.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "concurrently -k \"npm:dev:main\" \"npm:dev:preload\" \"npm:dev:renderer\" \"npm:electron\"",
    "dev:renderer": "vite",
    "dev:main": "tsc --watch --project electron/tsconfig.main.json",
    "dev:preload": "tsc --watch --project electron/tsconfig.preload.json",
    "electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "npm run build:renderer && npm run build:main && npm run build:preload",
    "build:renderer": "vite build",
    "build:main": "tsc --project electron/tsconfig.main.json",
    "build:preload": "tsc --project electron/tsconfig.preload.json",
    "lint": "eslint \"{src,electron}/**/*.{ts,tsx}\"",
    "preview": "npm run build && electron .",
    "test": "vitest run"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "electron": "^28.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wavefile": "^11.0.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.4.0",
    "wait-on": "^7.0.1"
  }
}
